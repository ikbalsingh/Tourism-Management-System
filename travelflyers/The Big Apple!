    <script>jQuery("#Flyer-Description").fitText(1.6, {minFontSize: '10px', maxFontSize: '18px'});</script>

    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBaB_9hgeARViVKIT6O1pFKXRCSuYaol2A&amp;libraries=places&amp;callback=initAutocomplete"></script>

    <script>
        function initAutocomplete() {

            var lat = 40.7127837000;
            var lng = -74.0059413000;

            var map = new google.maps.Map(document.getElementById('map'), {
                center: {
                    lat: lat,
                    lng: lng
                },
                zoom: 10,
                mapTypeId: google.maps.MapTypeId.ROADMAP
            });

            var marker = new google.maps.Marker({
                position: {
                    lat: lat,
                    lng: lng
                },
                map: map,
                animation: google.maps.Animation.DROP
            });
        }
    </script>

